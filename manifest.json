{
    "browser_specific_settings": {
        "gecko": {
            "data_collection_permissions": {
                "required": []
            },
            "id": "flavortown-github-exporter@hackclub.com",
            "strict_min_version": "140.0"
        }
    },
    "content_scripts": [
        {
            "js": ["content/content.js"],
            "matches": ["https://flavortown.hackclub.com/projects/new*"],
            "run_at": "document_end"
        }
    ],
    "description": "Import your GitHub projects to Flavortown with one click",
    "host_permissions": [
        "https://api.github.com/*",
        "https://flavortown.hackclub.com/*"
    ],
    "icons": {
        "128": "icons/icon128.png",
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png"
    },
    "manifest_version": 3,
    "name": "Flavortown GitHub Exporter",
    "permissions": ["storage"],
    "version": "1.5.0",
    "web_accessible_resources": [
        {
            "matches": ["https://flavortown.hackclub.com/*"],
            "resources": ["content/injected.js"]
        }
    ]
}
